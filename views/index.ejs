<!DOCTYPE html>
<html ng-app="project4">
<head>
  <title>FindACar</title>

  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBxM36DxD-TmmCuWMML0UXBVSirOUkF42Q"></script>
  <script src="https://js.stripe.com/v2/"></script>
  <script src="/vendor/angular/angular.min.js"></script>
  <script src="/vendor/angular-jwt/dist/angular-jwt.min.js"></script>
  <script src="/vendor/angular-resource/angular-resource.min.js"></script>
  <script src="/vendor/angular-ui-router/release/angular-ui-router.min.js"></script>
  <script src="/vendor/angular-bootstrap/ui-bootstrap.min.js"></script>
  <script src="/vendor/angular-bootstrap/ui-bootstrap-tpls.min.js"></script>


  <script src="/js/app.js"></script>
  <script src="/js/controllers/cars.js"></script>
  <script src="/js/controllers/users.js"></script>
  <script src="/js/controllers/navBarController.js"></script>
  <script src="/js/controllers/paymentController.js"></script>
  <script src="/js/models/car.js"></script>
  <script src="/js/models/user.js"></script>
  <script src="/js/services/authInterceptor.js"></script>
  <script src="/js/services/token.js"></script>
  
  <link href='https://fonts.googleapis.com/css?family=Roboto:400,100,300,500,700' rel='stylesheet' type='text/css'>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
  <link rel="stylesheet" type="text/css" href="css/style.css">

</head>
<body ng-controller="CarsController as cars">
  <div ng-controller="usersController as users">
    <nav ng-controller="navBarController as nav" class="navbar navbar-default">
      <div class="container-fluid">
        <div class="navbar-header">
          <a class="navbar-brand" ui-sref="home">FindaCar</a>
        </div>

        <ul class="nav navbar-nav navbar-right">
          <li><a ui-sref="addCar">Add a Car</a></li>
          <li><a ui-sref="allCars">All Cars</a></li>
          <li><a ui-sref="register">Sign up</a></li>
          <li><a ui-sref="login">Log in</a></li>
          <li><a href="#" ng-click="users.logout()">Log out</a></li>
      
          <span class="dropdown" uib-dropdown on-toggle="toggled(open)">
            <a href="simple-dropdown" uib-dropdown-toggle>
              <img ng-src="{{ users.currentUser.profileImage }}" class="profile-image img-circle">  {{ users.currentUser.username }}â–¾
            </a>
            <ul class="dropdown" uib-dropdown-menu aria-labelledby="simple-dropdown">
              <li ng-repeat="item in nav.accountItems">{{ item.name }}</li>
            </ul>
          </span>
        </ul>
      </div>
    </div>
  </nav>

  <main ui-view class="container"></main>

</body>
</html>